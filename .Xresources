URxvt.scrollBar: false
! URxvt.background: [100]#000000
! URxvt.foreground: [100]#ddccbb
URxvt.font: xft:PragmataPro:pixelsize=14
URxvt.boldFont: xft:PragmataPro-B:pixelsize=14
URxvt.italicFont: xft:PragmataPro-I:pixelsize=14
URxvt.boldItalicFont: xft:PragmataPro-Z:pixelsize=14

! Xft ----------------
Xft.autohint:  0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting:   1
Xft.antialias: 1
Xft.dpi:       96
Xft.rgba:      rgb

